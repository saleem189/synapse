{
  "name": "chat-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node backend/server.js",
    "worker": "tsx backend/worker.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\" \"npm run worker\"",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:fresh": "prisma migrate reset",
    "db:migrate:fresh-seed": "prisma migrate reset --force && npx tsx prisma/seed.ts",
    "db:status": "prisma migrate status",
    "db:fix-roles": "node scripts/fix-roles.js",
    "db:check-roles": "node scripts/check-roles.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "npm run docker:up && ping -n 6 127.0.0.1 >nul && npm run db:generate && npm run db:push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server ANALYZE=true next build --webpack",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser ANALYZE=true next build --webpack",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
